generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma/client" // required in Prisma 7
}

datasource db {
  provider = "postgresql"
}

model Channel {
  id String @id @default(uuid())
  name String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  workspaceId String
  createdById String 

  @@unique([workspaceId, name])
}
